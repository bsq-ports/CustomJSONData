{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.schema.json",
  "version": "0.4.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "CustomJSONData",
    "id": "custom-json-data",
    "version": "0.24.3",
    "url": "https://github.com/StackDoubleFlow/CustomJSONData",
    "additionalData": {
      "overrideSoName": "libcustom-json-data.so",
      "compileOptions": {
        "cppFlags": [
          "-DRAPIDJSON_NEON"
        ]
      },
      "cmake": true
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
      ],
      "log": [
        "pwsh ./scripts/copy.ps1 -log"
      ],
      "ndk-stack": [
        "pwsh ./scripts/ndk-stack.ps1"
      ]
    },
    "ndk": "^27.2.12479018",
    "qmodIncludeDirs": [
      "./build",
      "./extern/libs"
    ],
    "qmodIncludeFiles": [],
    "qmodOutput": "custom-json-data.qmod"
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.4.2",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "4007.*",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.18.3",
      "additionalData": {
        "includeQmod": true
      }
    },
    {
      "id": "songcore",
      "versionRange": "^1.1.23",
      "additionalData": {
        "includeQmod": true,
        "private": true
      }
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.6",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.6.4",
      "additionalData": {}
    },
    {
      "id": "cpp-semver",
      "versionRange": "^0.1.2",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "sombrero",
      "versionRange": "^0.1.43",
      "additionalData": {
        "private": true
      }
    }
  ]
}